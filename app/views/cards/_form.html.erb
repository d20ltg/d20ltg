<br>
  <%= semantic_form_for(@card, :html => { :multipart => true }) do |f| %>
    <%= f.inputs do %>
      <%= f.input :name %>
      <%= f.input :card_type %>
      <%= f.input :expansion_id, :as => :select, :collection => Hash[Expansion.all.map { |expac| [expac.set_name, expac.id] }] %>
     
    <% end %>

    <%= f.action :submit, as: :button, :disable_with => "Saving..." %>
  <% end %>
<br>
