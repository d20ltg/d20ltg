<br>
  <%= semantic_form_for(@expansion, :html => { :multiport => true }) do |f| %>
    <%= f.inputs do %>
	    <%= f.input :set_name %>
    <% end %>

    <%= f.hidden_field :set_abbreviation %>

    <%= f.input :block_id, :as => :select, :collection => Hash[Block.all.map { |block| [block.block_name, block.id] }] %>

    <%= f.label :image %>
    <%= f.file_field :image %>

    <br />
    <%= f.action :submit, as: :button, :disable_with => "Saving..." %>
  <% end %>
<br>

