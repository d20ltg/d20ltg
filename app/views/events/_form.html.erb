<br>
  <%= semantic_form_for(@event, :html => { :multiport => true }) do |f| %>
    <%= f.inputs do %>
      <%= f.input :title %>
      <%= f.input :description, :input_html => { :rows => 5 } %>
      <%= f.input :event_time %>
      <%= f.input :event_day, :as => :date_picker, :input_html => {:id => "day_picked"} %>
      <%= f.input :event_type %>
    <% end %>


    <fieldset class="form-group">
      <input type="submit" value="Submit" class="btn btn-primary">
    </fieldset>
  <% end %>
<br>
